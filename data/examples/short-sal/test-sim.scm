
(sal/import "short")
(start-simulation! "main")
(display-curr-states)
(select-state! 1)
(display-curr-states)
(step!)
(display-curr-trace)
(filter-curr-states! "(not request)")
(display-curr-states)
(define (n-step! n)
	(when (> n 0)
		(select-state! 1)
		(step!)
		(n-step! (- n 1))))
(n-step! 3)
(display-curr-trace)
(print "simulator worked")